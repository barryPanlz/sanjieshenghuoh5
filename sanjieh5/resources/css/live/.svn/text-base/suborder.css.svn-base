header{
    background: #12ac9f;
}
.sub_top{
	width: 100%;
	height: 1.1rem;
	background: #FFFFFF;
	margin-top:0.1rem ;
}
.sub_top_left{
	width:  87%;
	height: 1.1rem;
	float: left;
}
.sub_top_left_1{
	width:  87%;
	height: 1.1rem;
	float: left;
	font-size: 0.28rem;
	line-height: 1.1rem;
	text-indent: 0.3rem;
}
.sub_top_right{
	width: 10%;
	height: 1.1rem;
	float: left;
	text-align: center;
	line-height: 0.9rem;
}
.sub_top_left li{
	width: 100%;
	height: 0.53rem;
	float: left;
	text-indent:0.32rem ;
	font-size: 0.26rem;
	color: #282828;
	line-height: 0.53rem;
}
.sub_top_left li span{
	float: left;
}
.sub_top_left li img{
	width: 0.22rem;
	height: 0.28rem;
	float: left;
	margin-left:0.3rem ;
	margin-top:0.1rem ;
}
/*中间*/
.sub_center{
	position: absolute;
	top: 1.4rem;
	left: 0;
	bottom: 0rem;
	width: 100%;
	overflow: hidden;
	overflow-y: scroll;
}
.sub_center ul{
	width: 100%;
	float: left;
	overflow-x: hidden;
	background: #ffffff;
}
.sub_center ul li{
	width: 100%;
	height: 1.6rem;
	float: left;
	padding:0.2rem 0.3rem;
	border-bottom:1px solid #D4D4D4 ;
}
.sub_li_left{
	width: 25%;
	height: 1.6rem;
	float: left;
}
.sub_li_left img{
	width: 100%;
	height: 100%;
}
.sub_li_right{
	width: 62%;
	height: 1.6rem;
	float: left;
	margin-left:0.2rem ;
}
.sub_li_right p:nth-of-type(1){
	width: 100%;
	height: 0.6rem;
	line-height: 0.3rem;
	float: left;
	font-size: 0.22rem;
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
}
.sub_li_right p:nth-of-type(2){
	width: 100%;
	min-height: 0.5rem;
	line-height: 0.30rem;
	font-size: 0.2rem;
	float: left;
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
	color: #848484;
	margin-top:0.05rem ;
}
.sub_li_right p span{
	color: #848484;
}
.sub_li_right p:nth-of-type(3){
	width: 100%;
	height: 0.5rem;
	line-height: 0.7rem;
	float: left;
}
.sub_li_right p b{
	font-size: 0.22rem;
	float: left;
	font-weight: 400;
}
.sub_li_right p i{
	font-style:normal ;
	color: #12ac9f;
}
.jiajian{
	width: 1.18rem;
	height: 0.4rem;
	float: right;
	border: 1px solid #D4D4D4;
	margin-top:0.06rem ;
}
.shopPrice{
	font-size: 0.2812ac9f;rem;
}
.jiajian span{
	display: block;
	float: left;
	width: 0.38rem;
	height: 0.4rem;
	border-right:1px solid #D4D4D4 ;
	font-size: 0.32rem;
	text-align: center;
	line-height: 0.4rem;
	font-style: normal;
	color: #999999;
}
.jiajian span:nth-of-type(2){
	border-right:0;
	color: #282828;
}
.jiajian input{
	display: block;
	border:0 ;
	border-right:1px solid #D4D4D4 ;
	width: 0.38rem;
	height: 0.4rem;
	float: left;
	font-size: 0.18rem;
	text-align: center;
	line-height: 0.45rem;
	color: #282828;
}
/*配送方式*/
.peisong_wrap{
	width: 100%;
	min-height: 1.22rem;
	float: left;
}
.peisong{
	width: 90%;
	height: 1rem;
	
	margin: 0 auto;
	margin-top:0.2rem ;
}
.sub_center .peisong>div{
	width: 100%;
	height: 0.4rem;
	line-height:0.4rem ;
	float: left;
	
}
.sub_center .peisong div span{
	font-size: 0.16rem;
	color: #848484;
	float: left;
}
.sub_center .peisong div:nth-of-type(2){
	border-bottom: 1px solid #D4D4D4;
}
.sub_center .peisong .heji{
	height: 0.8rem;
	line-height: 0.8rem;
	text-align: right;
	font-size: 0;
}
.sub_center .peisong .heji span,.sub_center .peisong .heji i{
	font-size: 0.22rem;
	float: none;
}
.sub_center .peisong .heji i{
	font-style: normal;
	color: #12ac9f;
}
.dingdananniu{
	width: 100%;
	height: 0.9rem;
	float: left;
}
.dingdan_heji{
	width: 50%;
	height: 0.9rem;
	line-height: 0.9rem;
	text-indent: 0.32rem;
	font-size: 0;
	float: left;
}
.dingdan_heji span{
	font-size: 0.22rem;
	color: #575757;
}
.dingdan_heji i{
	font-size: 0.24rem;
	color: #12ac9f;
	font-style: normal;
	margin-left:0.08rem ;
}
.dingdan_queding{
	display: block;
	width: 50%;
	height:0.9rem;
	background: #12ac9f;
	color: #FFFFFF;
	text-align: center;
	line-height: 0.9rem;
	font-size: 0.28rem;
	background: #12ac9f;
	float: left;
}
/*使用规则*/
.shiyongguize{
	width: 100%;
	background: #FFFFFF;
	overflow: hidden;
	float: left;
	border-top: 5px solid #dddddd;
}
.shiy_top{
	width: 5.8rem;
	height: 0.62rem;
	border-bottom: 1px solid #dddddd;
	margin: 0 auto;
	font-size: 0.26rem;
	color: #575757;
	line-height: 0.62rem;
}
.shiyongguize>.ul{
	width: 5.8rem;
	margin: 0 auto;
	overflow: hidden;
	float: none;
}
.shiyongguize>.ul>p{
	width: 5.8rem;;
	line-height: 0.3rem;
	font-size: 0.2rem;
	color: #999999;
	padding: 0.05rem;
	
	
}

