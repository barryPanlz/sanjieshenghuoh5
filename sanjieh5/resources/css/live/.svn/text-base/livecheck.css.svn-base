.sk_top{
	width: 100%;
	min-height: 3.4rem;
	background: #FFFFFF;
}
.sk_top_top{
	width: 6rem;
	/*min-height: 2rem;*/
	padding: 0.2rem 0.13rem;
	border-bottom: 1px solid #CCCCCC;
	overflow: hidden;
	margin: 0 auto;
}
.sk_top_top>img{
	width: 2rem;
	height: 2rem;
	float: left;
}
.sk_top_top ul{
	width:3.6rem ;
	/*min-height: 2rem;*/
	float: left;
	margin-left:0.3rem ;
}
.sk_top_top ul li{
	width:100%;
	/*height: 0.32rem;*/
	line-height: 0.32rem;
	color: 575757;
	font-size: 0;
	float: left;
	overflow:hidden; 
	text-overflow:ellipsis;
}
.sk_top_top ul li:nth-of-type(1){
	font-size: 0.24rem;
	color: #282828;
	height: 0.65rem;
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
}
.sk_top_top ul li:nth-of-type(2){
	margin-top: 0.1rem;
}
.sk_top_top ul li span{
	/*width: 1.2rem;*/
	height: 0.32rem;
	overflow: hidden;
	font-size: 0.14rem;
	color: #585858;
	display: inline-block;
	float: left;
	
}
.sk_top_top ul li em{
	font-style: normal;
	color: #848484;
	font-size: 0.14rem;
	display: inline-block;
	max-width:2.8rem ;
	min-height: 0.32rem;
	overflow: hidden;
	text-overflow:ellipsis;
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
}
.sk_top_bot{
	width: 6rem;
	min-height: 1.8rem;
	padding: 0.2rem 0.13rem;
	overflow: hidden;
	margin: 0 auto;
}
.sk_top_bot ul{
	width:100%;
	height: 1.6rem;
	float: left;
}
.sk_top_bot ul li{
	width: 100%;
	height: 0.5rem;
	line-height: 0.5rem;
	font-size: 0.18rem;
	float: left;
	color: #575757;
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:1; 
}
.sk_top_bot ul li:nth-of-type(2n){
	color: #848484;
}
.sk_top_bot>div{
	width: 3rem;
	min-height: 1.6rem;
	float: left;
}
.sk_top_bot>div>li{
	width: 1.8rem;
	min-height: 1.6rem;
	margin-left:1.27rem ;
	
}
.sk_top_bot div img{
	width: 1.8rem;
	height:1.8rem;
}
.sk_top_bot div p{
	font-size: 0.18rem;
}
/*center*/
.sk_center_head{
	width: 100%;
	height: 0.8rem;
	background: #f6f6f6;
	text-align: center;
	font-size: 0.25rem;
	line-height: 0.8rem;
	color: #585858;
}
.sk_center_wrap{
	width: 100%;
	height: auto;
	overflow: hidden;
}
.sk_center_img{
	width: 100%;
	height: auto;
	overflow: hidden;
}
.sk_center_img img{
	height: 4rem;
}
.sk_center_conternt{
	width: 6rem;
	line-height: 0.32rem;
	font-size: 0.18rem;
	color: #575757;
	background: #FFFFFF;
	padding: 0.2rem ;
	
}
.sk_center_conternt img{
	width: 0.22rem;
	margin:0 0.05rem 0 0.08rem;
}
.sk_center_ul_wrap{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-bottom:0.2rem ;
}
.sk_center_ul_wrap li{
	width: 6.0rem;
	min-height: 2rem;
	padding: 0.2rem;
	border-bottom:1px solid #f6f6f6 ;
	background: #FFFFFF;
}
.sk_center_ul_wrap li img{
	width: 2rem;
	height: 2rem;
	float: left;
}
.sk_center_ul_wrap .li_right{
	width: 3.8rem;
	min-height: 2rem;
	float: left;
	margin-left:0.2rem ;
}
.cont_title{
	width: 100%;
	height:0.46rem;
	line-height: 0.46rem;
	font-size: 0.24rem;
	font-weight: 400;
	color: #282828;
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:1; 
}
.cont_content{
	width: 100%;
	height: 0.6rem;
	line-height: 0.3rem;
	font-size: 0.16rem;
	color: #848484;
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
}
.cont_mony{
	width: 100%;
	height: 0.4rem;
	padding: 0.1rem 0;
}
.cont_mony span{
	color: #12ac9f;
	float: left;
}
.cont_mony span:nth-of-type(1){
	font-size: 0.2rem;
	margin-top:0.05rem ;
	
}
.cont_mony span:nth-of-type(2){
	font-size: 0.3rem;
}
.cont_bott{
	width: 100%;
	height: 0.3rem;
	
}
.cont_bott em{
	font-style: normal;
	float: left;
	font-size: 0.2rem;
	color: #999999;
}
.cont_bott span{
	font-size: 0.2rem;
	color: #f3001d;
}
.cont_bott del{
	font-size: 0.2rem;
	color: #f3001d;
}
.cont_bott i{
	font-style: normal;
	font-size: 0.28rem;
	color: #f3001d;
}
.cont_bott b{
	font-size: 0.2rem;
	float: right;
	font-weight: 400;
}
.cont_bott b span{
	color: #999999;
}
.cont_bott b b{
	margin-left:0.1rem ;
}




.empty_list{
  width: 100%;
}
.empty_list img{
	display: block;
	width: 1.7rem;
	height: 1.48rem;
	margin: 0 auto;
	margin-top: 0.4rem;
}
.empty_list p{
	width: 100%;
	text-align: center;
	font-size: 0.2rem;
	color: #999999; 
}
				
