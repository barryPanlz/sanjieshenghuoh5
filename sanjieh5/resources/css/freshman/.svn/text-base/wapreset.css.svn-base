/* 清除内外边距 */
body, h1, h2, h3, h4, h5, h6, hr, p, /* structural elements 结构元素 */
dl, dt, dd, ul, ol, li, /* list elements 列表元素 */
fieldset, button, input, textarea, /* form elements 表单元素 */
th, td,img,section,div,span, a,header,section{ /* table elements 表格元素 */
    margin: 0;
    padding: 0;
    color: #333;
    list-style: none;
    -webkit-tap-highlight-color:transparent; 
    font-family: '微软雅黑';
}

h1, h2, h3, h4, h5, h6{ font-weight: 100; color: #292929;}
h1 { font-size: 0.32rem; /* 18px / 12px = 1.5 */ }
h2 { font-size: 0.26rem; }
h3 { font-size: 0.18rem; }
h4, h5, h6 { font-size: 100%;}

img{vertical-align:middle;}

/* 重置文本格式元素 */
a { text-decoration: none; -webkit-tap-highlight-color:transparent; -webkit-user-select: none;-moz-user-focus: none;-moz-user-select: none; color: #000;}
a:hover { text-decoration: none; }

/* 重置表单元素 */
fieldset, img { border: none; } /* img 搭车：让链接里的 img 无边框 */
button, input, select, textarea {/* 使得表单元素在 ie 下能继承字体大小 */
    font-size: 100%;
    border: 0;
    background: none;
    outline: none;
}

/*
 * ios 
 */
input[type="submit"],
input[type="reset"],
input[type="button"],
button {  
  -webkit-appearance: none;
  -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}


