/***************************************供应商-提现*****************************/
input{
	outline: none;
	-webkit-appearance: none;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
 input[type="text"]:focus{line-height: normal}
 input[type="number"]:focus{line-height: normal}

/*供应商提现方式*/
.supplier_withdrawal_style{
	width: 5.8rem;
	padding: 0 0.3rem;
	height: 7.6rem;
	margin-top: 0.14rem;
	background: #FFFFFF;
}
.supplier_withdrawal_style .supplier_withdrawal_style_h5{
	padding: 0.34rem 0 0.3rem 0;
	font-size: 0.24rem;
	color: #282828;
	width: 100%;
	font-weight: 400;
}
.supplier_withdrawal_style .supplier_withdrawal_style_num{
	display: block;
	font-size: 0.24rem;
	color: #575757;
	width: 5.7rem;
	padding-left: 0.1rem;
	height: 0.74rem;
	line-height: 0.74rem;
	border: 1px solid #CCCCCC;
	box-sizing: border-box;
	border-radius: 0.1rem;
	background: #FFFFFF;
}
.supplier_withdrawal_style li{
	width: 100%;
	font-size: 0.24rem;
	height: 0.74rem;
	line-height: 0.74rem;
	margin-top: 0.25rem;
	position: relative;
}
.supplier_withdrawal_style .supplier_withdrawal_style_li{
	margin-top: 0;
}
.supplier_withdrawal_style li img{
	width: 0.3rem;
	height: 0.3rem;
}
.supplier_withdrawal_style li span{
	color: #282828;
	padding: 0 0.2rem;
}
.supplier_withdrawal_style li input{
	width: 4.16rem;
	padding-left: 0.1rem;
	height: 0.72rem;
	line-height: 0.72rem;
	color: #575757;
	border: 1px solid #CCCCCC;
	border-radius: 0.1rem;
	float: right;
}
.supplier_withdrawal_style li select{
	width: 4.3rem;
	padding-left: 0.1rem;
	height: 0.72rem;
	line-height: 0.72rem;
	color: #575757;
	border: 1px solid #CCCCCC;
	border-radius: 0.1rem;
	float: right;
}
.supplier_withdrawal_btn{
	margin-top: 0.7rem;
}

.supplier_withdrawal_p{
	font-size: 0.18rem;
	color: #848484;
	line-height: 0.3rem;
	width: 5.8rem;
	margin: 0 auto;
}
.supplier_withdrawal_p1{
	margin-top: 0.34rem;
}
.supplier_withdrawal_p2{
	padding-bottom: 0.34rem;
	color: #848484;
}







/***************************************供应商-提现成功***************************************8*/
/*供应商提现成功图像*/
.supplier_withdrawal_prompt{
	margin-top: 0.14rem;
	width: 100%;
	text-align: center;
	height: 2.1rem;
	background: #FFFFFF;
}
.supplier_withdrawal_prompt img{
	width: 0.83rem;
	height: 0.83ren;
}
.supplier_withdrawal_prompt p{
	width: 100%;
	text-align: center;
	font-size: 0.28rem;
	color: #F01C1C;
}
/*供应商提现成功详细信息*/
.supplier_withdrawal_message{
	margin-top: 0.14rem;
	width: 5.8rem;
	padding: 0.16rem 0.3rem 0;
	height: 2.2rem;
	background: #FFFFFF;
}
.supplier_withdrawal_message li{
	width: 100%;
	font-size: 0.24rem;
	margin-top: 0.28rem;
	color: #282828;
}
.supplier_withdrawal_message li font{
	margin-left: 0.2rem;
	color: #848484;
}
/*供应商提现成功到账时间*/
.supplier_withdrawal_time{
	margin-top: 0.14rem;
	height: 0.48rem;
	line-height: 0.48rem;
	width: 5.8rem;
	padding: 0 0.3rem;
	background: #FFFFFF;
	font-size: 0.18rem;
	color: #282828;
}
.supplier_withdrawal_return{
	position: absolute;
	bottom: 0.2rem;
	width: 5.8rem;
}


/*列表*/
.store_tranfer_list {
	width: 100%;
	min-height: 0.5rem;
	height: auto;
	overflow: hidden;
	margin-top:0.6rem ;
}
.store_tranfer_list li{
	width: 100%;
	min-height: 0.5rem;
	overflow: hidden;
	
}
.store_tranfer_list .list_title{
	background: #DEDEDE;
	height: 0.5rem;
}
.store_tranfer_list .list_title p{
	height: 0.5rem;
	line-height: 0.5rem;
}
.store_tranfer_list li p{
	width: 33%;
	min-height: 0.8rem;
	line-height: 0.8rem;
	overflow: hidden;
	float: left;
	font-size: 0.2rem;
	color: #282828;
	text-indent: 0.3rem;
}



