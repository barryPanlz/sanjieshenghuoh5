	height: ;
}
header .u_shuaxin{
	background: url(../../images/kejiaohuan.png) no-repeat right center;
    -webkit-background-size: 0.48rem;;
}

.sor_top{
	width: 100%;
	height: 0.6rem;
	background: #ffff;
	font-size: 0;
	background: #FFFFFF;
	margin-top:0.16rem ;
}
.sor_top a{
	width: 25%;
	height: 0.6rem;
	display: block;
	float: left;
	text-align: center;
	line-height: 0.6rem;
}
.sor_top a span{
	font-size: 0.22rem;
	color: #282828;
}
.sor_top .color_red span{
	color: #F3001D;
}

/*中间*/
.sor_center{
	width: 100%;
	position: absolute;
	top: 0.76rem;
	bottom: 0;
	overflow-x: hidden;
	overflow-y: scroll;
	padding-bottom:0.4rem;
	
}
.sor_ce_order{
	width: 100%;
	min-height: 4rem;
	margin-top:0.14rem ;
	background: #ffffff;
	float: left;
}
.sor_ce_order_top{
	width: 100%;
	height: 0.56rem;
	line-height: 0.56rem;
	text-indent: 0.3rem;
	font-size: 0;
	background: #FFFFFF;
}
.sor_ce_order_top span{
	font-size: 0.18rem;
	color: #848484;
	float: left;
}
.sor_ce_order_top span:nth-of-type(2){ 
	width: 4.78rem; 
    text-overflow:ellipsis;
    white-space:nowrap; 
    overflow:hidden;
    text-indent: 0;
}
/*.sor_ce_order_top span:nth-of-type(3){ 
	width: 1.54rem;
	margin-right:0.2rem ;
    float: right;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}*/
.sor_ce_order_ul{
	width: 100%;
	min-height: 1.8rem;
	font-size: 0;
	border-top: 1px solid #D4D4D4;
}
.sor_ce_order_ul li{
	width: 100%;
	height: 1.4rem;
	float: left;
	border-bottom:2px solid #FFFFFF ;
	padding: 0.2rem 0;
	background: #f6f6f6;
	
	
}
.sor_ce_order_ul li img{
	width: 1.4rem;
	height: 1.4rem;
	float: left;
	margin-left:0.3rem ;
}
.sor_ce_li_right{
	width: 62%;
	height: 1.4rem;
	float: left;
	margin-left:0.25rem ;
}
.sor_ce_li_right p:nth-of-type(1){
	width: 100%;
	height: 0.6rem;
	float: left;
	line-height: 0.3rem;
	font-size: 0.24rem;
	color: #575757;
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
}
.sor_ce_li_p2{
	width: 100%;
	height: 0.4rem;
	float: left;
	line-height: 0.4rem;
	color: #575757;
	margin-top:0.05rem ;
}
.sor_ce_li_p2 span{
	font-size: 0.18rem;
}
.sor_ce_li_p2 span:nth-of-type(2n){
	color: #999999;
}
.sor_ce_li_p2 span:nth-of-type(3){
	margin-left:0.25rem ;
}
.sor_ce_li_p3{
	width: 100%;
	height: 0.36rem;
	float: left;
	line-height: ;
	font-size: 0;
}
.sor_ce_li_p3 span{
	
	color: #848484;
	float: left;
	font-size: 0.18rem;
}
.sor_ce_li_p3 span:nth-of-type(1){
	font-size: 0.22rem;
	color: #F3001D;
	margin-right:0.1rem ;
}
.sor_ce_li_p3 span:nth-of-type(2){
	margin-top:0.03rem ;
}
.sor_ce_li_p3 span:nth-of-type(3){
	float:right ;
}
.sor_ce_li_p3 i{
	font-style: normal;
	margin-left:0.05rem ;
}

.sor_ce_order_bottom{
	/*width: 6.1rem;*/
	height: 1.3rem;
	background: #FFFFFF;
	float: right;
	margin-right:0.2rem ;
	
}
.sor_ce_order_bottom p{
	width: 100%;
	height: 0.4rem;
	line-height: 0.4rem;
	text-align: right;
	font-size: 0;
	margin-top:0.2rem ;
}
.sor_ce_order_bottom p span{
	font-size: 0.18rem;
	
}
.sor_ce_order_bottom p i{
	font-size: 0.18rem;
	font-style:normal ;
	color: #F3001D;
	margin:0 0.1rem ;
}
.sor_ce_order_bottom p .big_i{
	font-size: 0.22rem;
}
.sor_ce_order_bottom li{
	width: 100%;
	height: 0.76rem;
	margin-top:0.1rem ;
}
.sor_ce_order_bottom li span{
	float: right;
}
.sor_ce_order_bottom li span i{
	font-size: 0.24rem;
	float: left;
	font-style: normal;
	color: #f3001d;
	margin-top:0.1rem ;
	margin-right:0.16rem ;
}
.sor_ce_order_bottom li button{
	width: 1.22rem;
	height: 0.42rem;
	text-align: center;
	line-height: 0.42rem;
	font-size: 0.24rem;
	background: #F3001D;
	color: #FFFFFF;
	float: left;
	border-radius:2px ;
}
.sor_ce_order_bottom li button a{
	color: #FFFFFF;
}
/*模态框*/
.sr_model_wrap{
	display: none;
	width: 100%;
	height: 100%;
	background: rgba(1,1,1,0.5);
	position: absolute;
	top: 0;
	left: 0;
	z-index: 10000;
}
.sr_model{
	display: block;
	width: 4.8rem;
	/*height: 4rem;*/
	padding: 0.2rem 0.2rem 0rem 0.2rem;
}
.sr_model_wrap1{
	width: 5.2rem;
	min-height:4rem ;
	background: white;
	margin: 2.56rem auto;
	background: #FFFFFF;
	border-radius:0.1rem ;
}
.sr_model li:nth-of-type(1){
	width: 100%;
	height: 0.8rem;
	line-height: 0.8rem;
	font-size: 0.26rem;
	color: 0.28rem;
	text-align: center;
	
}
.sr_model_li{
	width: 100%;
	height: 1.13rem;
	
}
.sr_model_li span{
	float: left;
	font-size: 0.26rem;
	line-height: 1.13rem;
}
.sr_model_li select,.sr_model_li input{
	width: 3.4rem;
	height: 0.76rem;
	float: left;
	margin-left:0.25rem ;
	font-size: 0.26rem;
	margin-top:0.2rem;
	border: 1px solid #D4D4D4;
	border-radius:2px ;
	
}
.sr_model_li select option{
	font-size: 0.26rem;
	color: #585858;
}
.sr_model_sub_wrap{
	width: 100%;
	height: 0.76rem;
	margin-top:1.1rem;
	border-top: 1px solid #D4D4D4;
	font-size: 0;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
	overflow: hidden;
}
.sr_model_sub_wrap button{
	width: 50%;
	float:left ;
	height: 0.76rem;
	font-size:0.28rem ;
	color: #font-size;
	background: #FFFFFF;
}
.sr_model_sub_wrap button:nth-of-type(2){
	background: #F3001D;
	color: #FFFFFF;
	
}
.fun_model_wrap_al{
	display: none;
}


/*模态框位置*/
.motai_lose,.motai_succe{
	display: none;
	position: absolute;
	left: 1.4rem;
	top: 4rem;
	z-index: 100;
}



