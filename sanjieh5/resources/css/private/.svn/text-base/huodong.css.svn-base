
/*活动中心页面*/
.main{
	background: #f6f6f6;
}
.tab_box{
/*	padding:0 .3rem;*/
	overflow: hidden;
	background: #fff;
}
.tab_box a{
	position:relative;
	display: block;
	
}
.tab_box a h3{
	padding:0 .3rem;
	font-size:.24rem;
	color:#E01919;
	text-align: center;
	height:.6rem;
	line-height: .6rem;
}
.tab_box a ul{
	padding:0 .3rem;
	height:.6rem;
	line-height: .6rem;
	border-top:1px solid #f6f6f6;
}
.tab_box .tab_hid{
	display: none; 	
}
.tab_box a ul li{
	float:left;
	font-size:.24rem;
}
.tab_div1 li:first-child{
	width:16%;
}
.tab_div1 li:nth-child(2){
	width:24%;
	color:#282828;
}
.tab_div1 li:nth-child(3){
	width:30%;
}
.tab_div2 li:first-child{
	width:16%;
}
.tab_div2 li:nth-child(2){
	width:24%;
	color:#282828;
	
}
.tab_div2 li:nth-child(3){
	width:34%;
	white-space:nowrap; 
	overflow:hidden; 
	text-overflow:ellipsis;
}


.tab_num {
	height:.6rem;
	text-align: center;
	width:1.6rem;
	margin:0 auto;
}
.tab_num li{
	margin-top:.24rem;
	float:left;
	width:.5rem;
	height:.1rem;
	background: #ddd;
}
.tab_num li:first-child{
	margin-right:.3rem;
}
.tab_num .tab_active{
	background:#E01919;
}
.guanggao{
	margin-top:.15rem;
	background: #fff;
}
.guanggao h3{
	height:.6rem;
	font-size:.3rem;
	color:#484848;
	text-align: center;
	line-height: .6rem;
}
.guanggao img{
	width:100%;
}
.nav_type{
	overflow: hidden;
	background: #fff;
	margin:.15rem 0;
}
.nav_type ul li{
	float:left;
	width:33%;
	height:2.12rem;
}
.nav_type ul {
	text-align: center;
	overflow: hidden;
	border-bottom:1px solid #f6f6f6;
}
.nav_type ul li:first-child{
	border-right:1px solid #f6f6f6;
}
.nav_type ul li:nth-child(2){
	border-right:1px solid #f6f6f6;
}
.nav_type ul li a{
	display: block;
}

.nav_type ul li p{
	font-size:.24rem;
	color:#808080;
}




/*/--------------------排行榜页面c-------------------/*/
.paihang{
	background: #fff;
	height:.78rem;
	border-bottom:1px solid #F6F6F6;
	line-height: .78rem;
	width:100%;
}
.paihang a{
	float:left;
	font-size:.24rem;
	color:#282828;
	width:50%;
	text-align: center;
}
.paihang a:first-child{
	border-right:1px solid #ddd;
	margin-left:-1px;
}
.paihang .paihang_active{
	color:#F3001C;
	border-bottom: 2px solid #F3001C;
}
.paihang_box{
	width:100%;
}
.paihang_div1{
	width:100%;
	margin-top:.2rem;
	height:.5rem;
	background: #ddd;
	line-height: .5rem;
	padding:0 .15rem;
}

.paihang_div1 li{
	float:left;
	font-size:.22rem;
	color:#282828;
}
	
.paihang_div1 li:first-child{
	width:20%;
}
.paihang_div1  li:nth-child(2){
	width:23%;
	color:#282828;
}
.paihang_div1  li:nth-child(3){
	width:24%;
	color:#282828;
}
.paihang_div2 ul li{
	float:left;
	font-size:.22rem;
	color:#848484;
}
.paihang_div2 ul{
	padding:0 .15rem;
	height:.78rem;
	line-height: .78rem;
	border-bottom:1px solid #ddd;
}
.paihang_div2 ul li:first-child{
		width:16%;
}
.paihang_div2  li:nth-child(2){
	width:24%;
	
}
.paihang_div2 li:nth-child(3){
	width:30%;
	color:#282828;
}

.paihang_div22 li:nth-child(3){
	overflow:hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}



