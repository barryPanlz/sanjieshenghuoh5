/***************************升级创业会员成功*******************************/
.upgrade_suc_img{
	width: 100%;
	text-align: center;
	height: 2.1rem;
	background: #FFFFFF;
	margin-top: 0.14rem;
}
.upgrade_suc_img img{
	width: 0.8rem;
	height: 0.8rem;
}
.upgrade_suc_img p{
	width: 100%;
	text-align: center;
	font-size: 0.28rem;
	color: #f01c1c;
	margin-top: 0.16rem;
}

.upgrade_suc_return{
	margin-top: 0.2rem;
}

/*会员操作权限*/
.upgrade_suc_permis{
	width: 6.4rem;
	height: 0.6rem;
	line-height: 0.6rem;
	background: #DDDDDD;
	margin-top: 0.3rem;
	border: 1px solid #CCCCCC;
	box-sizing: border-box;
}
.upgrade_suc_permis li{
	float: left;
	font-size: 0.2rem;
	color: #282828;
	width: 2.12rem;
	text-align: center;
}
.upgrade_suc_odd{
	width: 6.4rem;
	height: 1.02rem;
	line-height: 1.02rem;
	background: #FFFFFF;  
}
.upgrade_suc_odd li{
	float: left;
	font-size: 0.18rem;
	color: #575757;
	width: 2.12rem;
	text-align: center; 
	position: relative;
	vertical-align: middle;
}
.upgrade_suc_odd li img{
	width: 0.5rem;
	height: 0.5rem;
	margin: auto;
}
.upgrade_suc_odd li p{ 
	width: 1.8rem;
	font-size: 0.18rem;
	line-height: 0.24rem;
	color: #575757;
	display: inline-block;
}
.upgrade_suc_even{
	width: 6.4rem;
	height: 1.02rem;
	line-height: 1.02rem;
	background: #FFFFFF;  
}
.upgrade_suc_even li{
	position: relative;
	float: left;
	font-size: 0.18rem;
	background: #F7F7F7;
	width: 2.12rem;
	height: 1.02rem;
	line-height: 1.02rem;
	text-align: center;  
	/*vertical-align: middle;*/
}
.upgrade_suc_even li p{
	margin: auto auto;
	width: 1.8rem;
	font-size: 0.18rem;
	line-height: 0.24rem;
	color: #575757;
	/*display: inline-block;*/
	/*position: absolute;
	top: 50%;
	margin-top: -50%;*/
	position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.upgrade_suc_even li img{
	width: 0.5rem;
	height: 0.5rem;
	margin: auto auto;
}




/***************************升级创业会员*******************************/
/*升级创业会员说明文字*/
.upgrade_diamond_p{
	width: 5.8rem;
	min-height: 0.52rem;
	height: auto;
	padding: 0.2rem 0.3rem;
	font-size: 0.2rem;
	line-height: 0.26rem;
	color: #848484;
}
/*升级创业会员升级费用*/
.upgrade_diamond_cost{
	width: 5.8rem;
	padding: 0 0.3rem;
	min-height: 2.5rem;
	background: #FFFFFF;
}
.upgrade_diamond_fee{
	padding-top: 0.3rem;
	font-size: 0.22rem;
}
.upgrade_diamond_fee span{
	color: #282828;
}
.upgrade_diamond_fee strong{
	color: #F3001D;
	font-size: 0.24rem;
	font-weight: 500;
	margin-left: 0.3rem;
} 
.upgrade_diamond_fee font{
	color: #999;
	font-size: 0.18rem; 
} 
.upgrade_diamond_way{
	width: 100%;
	height: 1.6rem;
	margin-top: 0.3rem;
}
.upgrade_diamond_way p{
	color: #282828;
	font-size: 0.22rem;
}
.upgrade_diamond_way li{
	width: 100%;
	float: left; 
	font-size: 0;
	margin-top: 0.2rem;
	line-height: normal;
}
.upgrade_diamond_way li img:first-child{
	width: 0.24rem;
	height: 0.24rem;
	float: left;
}
.upgrade_diamond_way li img:nth-child(2){
	width: 1rem;
	margin-left: 0.3rem;
	float: left;
}
.upgrade_diamond_way li font{
	font-size: 0.18rem;
	color: #999999;
	float: left;
	line-height: 0.38rem;
	margin-left: 0.1rem;
}


.upgrade_diamond_fail{
	position: fixed;
	top: 50%;
	margin-top: -0.85rem;
	left: 50%;
	margin-left: -1.85rem;
}






























