/*文本区域*/
.d_info{
	position:relative;
	height:3rem;
	background:#fff;
}
.d_top_box{
	width:95%;
	margin:.1rem auto;
	
}
.d_top{
	height:.79rem;
	padding-top:.19rem;
}
.d_top p{
	font-size:.24rem;
	color:#575757;
	font-weight: 100;
}
.d_mid{
	height:1.02rem;
	border-bottom: 1px solid #ccc;
}
.d_mid_top{
	height:.52rem;
	font-size:0;
}
.d_mid_top span{
	display: inline-block;
}
.d_mid_top .s{
	color:#F3001D;
}
.d_mid_top .t{
	font-size:.32rem;
}
.d_mid_top .b{
	margin-left:.08rem;
	font-size:.2rem;	
}
.d_mid_top .vip{
	margin-left:.08rem;
	height:18px;
	width:58px;
	min-width:.58rem;
	line-height:18px;
	text-align: center;
	font-size:12px;
	color:#fff;
	background: #F3001D;
	border-radius: 3px;
	vertical-align:top;
}	
.d_mid_mid{
	height:.47rem;
}
.d_mid_mid p{
	float:left;
	width:2.02rem;
	height:.47rem;
	line-height: .47rem;
	font-size:.18rem;
	color:#999;
}
.d_mid_mid p span{
	color:#575757;
}
.d_mid_mid .two{
	text-align: center;
}
.d_mid_mid .three{
	text-align: right;
}
.d_btm{
	height:.94rem;
}
.d_btm_top,.d_btm_mid{
	clear: both;
	height:.47rem;
	line-height: .47rem;

}
.d_btm_top p,.d_btm_mid p,.d_btm_top span,.d_btm_mid span{
	font-size:.18rem;
	color:#999;
}

.d_btm_top .fl{
	float:left;
}
.d_btm_top .fr{
	float:right;
	width:2rem;
	overflow: hidden;
	 white-space:nowrap;
	 text-overflow: ellipsis;
}
.d_btm_mid span{
	color:#F3001D;
}
/*文本区结束*/

.d_tit h3{
	height:.8rem;
	width:100%;
	background: #f6f6f6;
	text-align: center;
	line-height: .8rem;
	font-size:.26rem;
	color:#575757;
}
.d_tit img{
	width:100%;
}



/*底部选项*/
footer{
	clear: both;
	width:100%;
}
footer .fl{
	height:.85rem;
	width:100%;
	
}
footer .fl a{
	float:left;
	display: block;
	width:21.875%;
	height:.86rem;
	text-align: center;
}
footer .fl a img{
	display: block;
	width:.4rem;
	height:.4rem;
	margin:.23rem auto;
}
footer .fl input{
	float:left;
}

.add_cart,.buy_now{
	font-size:.28rem;
	color:#fff;
	border-radius: 0;
}
.add_cart{
	width:32.8%;
	height:100%;
	background: #f36100;
	text-align: center;
	line-height: .86rem;
}
.buy_now{
	width:23.4%;
	height:100%;
	background: #F3001D;
	text-align: center;
	line-height: .86rem;
}

/*购买添加尺寸*/
.mark{
	position:absolute;
	left:0;
	top:0;
	bottom:0;
	height:100%;
	width:100%;
	background: #000000;
	opacity: .3;
	z-index: 111;	
	display: none;
}
/*.opc{
	height:43%;
	width:100%;
	background: #000000;
	opacity: .2;
}*/

.c_box{
	position:absolute;
	z-index: 111;
	bottom:0;
	height:57%;
	width:100%;
	background: #fff;
	display: none;
}
.color_box,.rule_box,.num_box,.c_chu{
	width:95.3%;
	clear: both;
	margin:0 auto;
}

.c_box h3{
	margin:.3rem 0 .16rem 0;
	font-size:.22rem;
	color:#848484;
}
.c_box li:first-child{
	margin-left:0;
}
.c_box li{
	float:left;
	font-size:.22rem;
	color:#575757;
	width:1.24rem;
	height:.44rem;
	border:1px solid #ccc;
	text-align: center;
	line-height: .44rem;
	margin-left:.26rem;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	border-radius: 3px;
	margin-bottom:.3rem;
}
.c_box .rule_box li{
	margin-bottom:.6rem;
}
.c_box .c_3{
	border:1px solid #F3001D;
	color:#F3001D;
}
.num_box{
	clear: both;
	border-top:1px solid #ccc;
	height:.5rem;
	padding-top:.24rem;
	line-height: .5rem;
	margin-bottom: .1rem;
}
.num_box p{
	font-size:.22rem;
	color:#848484;
}
.num_box .fr{
	width:1.7rem;
	height:.48rem;
	border:1px solid #ccc;
	border-radius:3px;
}
.num_box p input{
	float:left;
	height:100%;
	width:.66rem;
	border:1px solid #ccc;
	border-top:0px;
	border-bottom:0px;
	text-align: center;
}
.num_box p span{
	float:left;
	height:.5rem;
	width:.5rem;
	text-align: center;
	line-height:.52rem;
	font-size:.32rem;
	color:#848484;
}
.num_box p .c28{
	color:#282828;
}
.num_box .fl{
	float:left;
}
.num_box .fr{
	float:right;
	
}
.c_chu p{
	float:right;
	font-size:.20rem;
	color:#999;
}


.sure{
	position:absolute;
	bottom:0;
	height:.84rem;
	width:100%;
	background: #F3001D;
	text-align: center;
	line-height: .84rem;
	color:#fff;
	z-index: 111;
	font-size:.28rem;
}


/*成功的弹出窗*/
.succ{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	z-index: 223;
	display: none;
}
.succ_box{
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-1.35rem;
	margin-top:-.99rem;
	width:2.7rem;
	height:1.9rem;
	background: url(../../images/tankuang-bg.png) no-repeat center;
	background-size:100%;
}
.succ_box img{
	display: block;
	margin:0 auto;
	padding-top:.4rem;
	width:.68rem;
	height:.48rem;
}
.succ_box p{
	font-size:.28rem;
	color:#fff;
	margin-top:.3rem;
	text-align: center;
}


