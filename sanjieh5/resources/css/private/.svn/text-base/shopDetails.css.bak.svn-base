/* main_image */
.main_visual{
	height:6.4rem;
	width:100%;
	border-top:1px solid #d7d7d7;
	overflow:hidden;
	position:relative;
}
.main_image{
	height:6.4rem;
	overflow:hidden;
	position:relative;
}
.main_image ul{
	width:9999px;
	height:6.4rem;
	overflow:hidden;
	position:absolute;
	top:0;left:0;
}
.main_image li{float:left;width:6.4rem;height:422px;}

.main_image li a{display:block;width:100%;height:422px}
.main_image li img{
	width:100%;
	/*height:.8rem;*/
}
div.flicking_con{
	font-size:0;
	position:absolute;
	bottom:.15rem;
	right:.15rem;
	z-index:999;
	width:.8rem;
	height:.8rem;
	border-radius: 50%;
	background:#e5e5e5 ;
	line-height: .8rem;
	text-align: center;
}
div.flicking_con a{
	margin-left:.1rem;
	float:left;
	font-size:.40rem;
	color:#fff;
	padding:0;
    display:none;
}
div.flicking_con span{
	font-size:.32rem;
	color:#fff;
}
div.flicking_con a.on{
	display:block;
}
