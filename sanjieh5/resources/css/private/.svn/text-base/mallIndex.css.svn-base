.main_1{
	overflow-x: hidden;
}
/*商城首页-总导航*/
.mallIndex_navi{
	width: 100%;
}
.mallIndex_navi a{
	float: left;
	width: 20%;
	height: 1.54rem;
	text-align: center;
	background: #FFFFFF;
}
.mallIndex_navi a img{
	width: 0.8rem;
	height: 0.8rem;
	display: block;
	margin: 0 auto;
	padding-top: 0.2rem;
}
.mallIndex_navi a p{
	display: block;
	font-size: 0.2rem;
	color: #575757;
	margin-top: 0.1rem;
}

/*合伙商*/
.partnerShip{
	width: 100%;
	height: auto;
	overflow: hidden; 
	font-size: 0;
	margin-top: 1.6rem;
}
.partnerShip a{
	float: left;
	width: 49.4%;
    height: 1.3rem;
    margin-bottom: 0.1rem;
}
.partnerShip_total a{
	float: left;
	width: 49.4%;
    height: 1.4rem;
    margin-bottom: 0.1rem;
}
.partnerShip a img{
	width: 100%;
	height: 100%;
}
.partnerShip a:nth-child(even){
    margin-left: 1.2%; 
}

/*商城首页-品质*/
.mallIndex_h3{
	clear: both;
	width: 100%;
	text-align: center;
	height: 0.6rem;
	line-height: 0.6rem;
	font-size: 0.24rem;
	color: #575757; 
}
.mallIndex_quality{
	width: 100%;
	height: auto;
	overflow: hidden;
}
.mallIndex_quality_ul1{
	float: left;
	width:  49.4%;
}
.mallIndex_quality_ul1 a{
	display: block;
	width: 100%;
	height: 4.5rem;
}
.mallIndex_quality_ul1 a img{
	width: 100%;
	height: 100%;
}
.mallIndex_quality_ul2{
	float: left;
	margin-left: 1.2%;
	width:  49.4%;
}
.mallIndex_quality_ul2 a{
	display: block;
	width: 100%;
	height: 2.2rem;
}
.mallIndex_quality_ul2 a img{
	width: 100%;
	height: 100%;
}
.mallIndex_quality_ul2 .mallIndex_quality_ul2_a{
	margin-top: 0.1rem;
}
.mallIndex_quality_dif{
	margin-top: 0.1rem;
}
.mallIndex_quality_dif .mallIndex_quality_ul2{
	margin-left: 0;
}
.mallIndex_quality_dif .mallIndex_quality_ul1{
	margin-left: 1.2%;
}

/*商城首页-未来生活*/
.mallIndex_life{
	clear: both;
	width: 100%;
	min-height: 3.4rem;
	height: auto;
	padding-bottom: 0.1rem;
}
.mallIndex_life li{
	float: left;
	width:  49.4%;
	min-height: 3.4rem;
	height: auto;
	padding-bottom: 0.1rem;
	background: #FFFFFF;
}
.mallIndex_life li a{
	display: block;
}
.mallIndex_life li img{
	display: block;
	width: 100%;
	height: 2.03rem;
}
.mallIndex_life .mallIndex_life_li1{
	margin-left: 1.2%;
}
.mallIndex_life li p{
	font-size: 0.24rem;
	padding: 0 0.16rem;
	width: 80%;
	color: #282828;
	margin-top: 0.2rem;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.mallIndex_life li font{
	display: block;
	font-size: 0.2rem;
	padding: 0 0.16rem;
	width: 80%;
	color: #8D8D8D;
	margin-top: 0.1rem;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.mallIndex_life li dt{
	padding: 0 0.16rem;
	width: 2.83rem;
}
.mallIndex_life li dt em{
	font-size: 0.28rem;
	float: left;
	color: #F3001D;
	font-style: normal;
}
.mallIndex_life li dt a{
	font-size: 0.2rem;
	float: right;
	width: 0.88rem;
	text-align: center;
	height: 0.36rem;
	line-height: 0.36rem;
	color: #F3001D; 
	background: #fef2f3;
	border-radius: 0.03rem;
	margin-top: 0.06rem;
}
.mallIndex_life_ul{
	margin-top: 0.2rem;
}

/*商城首页-广告*/
.mallIndex_ad{
	display: block;
	clear: both;
	width: 100%;
	height: 1.6rem; 
	margin-bottom: 0.1rem;
}
.mallIndex_ad img{
	width: 100%;
	height: 100%;
}

/*商城首页-商品列表*/
.mallIndex_list{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-top: 0.1rem;
}
.mallIndex_list a{
	float: left;
	width:  49.4%;
	height: 2.48rem;
	position: relative;
	margin-bottom: 0.1rem;
}
.mallIndex_list a:nth-child(even){
  margin-left: 1.2%;
}
.mallIndex_list a img{
	width: 100%;
	height: 100%;
}
.mallIndex_list .mallIndex_list_a{
	/*margin-left: 0.1rem;*/
}
.mallIndex_list a h4{
	position: absolute;
	z-index: 4;
	left: 0.16rem;
	top: 0.2rem;
	width: 2.83rem;
	font-size: 0.24rem;
	color: #282828;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.mallIndex_list a p{
	position: absolute;
	z-index: 4;
	left: 0.16rem;
	top: 0.54rem;
	width: 2.83rem;
	font-size: 0.2rem;
	color: #999;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.mallIndex_list a font{
	position: absolute;
	z-index: 4;
	left: 0.16rem;
	top: 0.94rem;
	width: 2.83rem;
	font-size: 0.28rem;
	color: #F3001D;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

 


  
/*商城首页轮播图*/
.mallIndex_img{
	width: 6.4rem;
	height: 2rem;
	overflow: hidden;
}
.mallIndex_img img{
	width: 100%;
	height: 2rem;
} 
.main_visual{
	width: 6.4rem;
	height: 2rem;
	overflow: hidden;
}
.main_image{
	width: 6.4rem;
	height: 2rem;
	overflow: hidden;
}
#main_image_ul{
	width: 100%;
	height: 2rem;
	overflow: hidden;
}
div.flicking_con{
	position:absolute;
	top:360px;
	left:50%;
	z-index:999;
	width:300px;
	height:21px;
	margin:0 0 0 -50px;
}
div.flicking_con a{
	float:left;
	width:21px;
	height:21px;
	margin:0;
	padding:0;
    display:none;
    background:url('../images/btn_main_img.png') 0 0 no-repeat;
    display:block;
    text-indent:-1000px;  
}
