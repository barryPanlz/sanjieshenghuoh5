/****************************未来生活首页****************************/ 
.life_index{
	height: 4.68rem;
	font-size: 0; 
}
.life_index a{
	float: left;
	width: 100%;
	height: 3.15rem;
	position: relative;
}
.life_index a img{
	width: 100%;
	height: 100%;
}
.life_index li p{
	padding-top: 0.2rem;
}
.life_index .life_index_em{
	display: block;
	padding: 0 0.16rem;
    width: 80%;
    font-size: 0.28rem;
	color: #12aca0;
	font-style: normal;
	margin-top: 0.1rem;
}
.life_index ol{
	padding: 0 0.16rem;
    width: 88%; 
    margin-top: 0.1rem;
}
.life_index li ol span{
	font-size: 0.2rem;
	color: #848484;
}
.life_index li ol strong{
	font-size: 0.16rem;
	color: #848484;
	font-weight: 200;
	float: right;
}
.life_index li ol i{
	font-style: normal;
	color: #282828;
}



















