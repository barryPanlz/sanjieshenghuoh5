}
.g_box .txt{
	font-size:.28rem;
}
.g_box .num{
	font-size:.48rem;
}
.g_box .num,.g_box .txt{
	color:#fff;
}
.g_box ul li{
	width:99.9%;
}
.g_withdraw{
	font-size:0;
	text-align: center;
	line-height: .6rem;
}
.g_withdraw a{
	font-size:.28rem;
	color:#F3001D;
	
}
.s_list .z_tit li,.s_li ul li{
	width:33.33%;
}
/*供应商我的账房页面结束*/




