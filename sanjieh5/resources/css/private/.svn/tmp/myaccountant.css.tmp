  
.fix:after{
	display: block;
	content:'';
	clear: both;
	height: 0;
	visibility: visible;
}
.fix{
	*zoom:1;
}
header{
	background: #fff;
}
header .u_back{
	background: url(../../images/rightarrow.png) no-repeat center left;
	background-size: 0.4rem;
}
header h1{
	color:#282828;
	font-size:.32rem;
}

/*顶部的余额板块*/
.b_box{
	margin: .15rem 0;
	padding:.5rem 0 .5rem 0;
	height:1.2rem;
	background: #F3001C;
}
.b_box ul .border_active:after{
	content: '';
	position: absolute;
	top:.05rem;
	right:.01rem;
	height:1.01rem;
	border:1px solid #C10F10;
	z-index: 11;
}
.b_box ul li{
	position:relative;
	float:left;
	width:50%;
	text-align: center;
}
.b_box ul li .txt{
	font-size:.28rem;
	color:#fff;
}
.b_box ul li .num{
	display: block;
	font-size:.48rem;
	color:#fff;
	margin-top:.2rem;
}
.b_box ul li .why{
	display: block;
	font-size:.24rem;
	color:#fff;
}
.re_box{
	height:1.4rem;
}

/*充值板块*/
.z_box{
	height:.6rem;
	padding:.14rem 0px;
	background: #fff;
}

.z_box ul li{
	font-size:0;
	float:left;
	border-left:1px solid #c9c9c9;
	width:33%;
	height:.6rem;
	line-height: .6rem;
	text-align: center;

}
.z_box ul li img{
	width:.14rem;
	height:.08rem;
}

.z_box ul li a{
	color:#F3001D;
	font-size:.28rem
}
.z_box select{
	width:100%;
	height:.6rem;
	line-height: .6rem;
	text-align: center;
	font-size:.28rem;
	color:#F3001D;
}
.z_box option{
	width:100%;
	height:.6re
	font-size:.28rem;
	color:#F3001D;
}
.z_box ul li:first-child{
	border-left:1px solid #fff;
}
/*增加转账*/
.trans_num{
	position:relative;
}
.trans_num span{
	display: block;
	width:80%;
	font-size:.28rem;
	color:#F3001D;
	margin-right:10px;
}
.trans_num img{
	position: absolute;
	top:.3rem;
	right:.4rem;
}
.trans_num div{
	display: none;
	width: 100%;
	position: absolute;
	left:0;
	z-index: 11;
	background: #fff;
	box-shadow: 1px 2px 4px #8C8C8C;
	border-radius: 3px;
}
.trans_num a{
	display: block;
}
.trans_num a:first-child{
	border-bottom:1px solid #f6f6f6;
}


/*增加分页*/
.page_book{
	width:60%;
	margin:.2rem auto;
	display: block;
	font-size:0;
	clear: both;
}
.page_book a{
	height:.4rem;
	width:1.2rem;
	font-size:.20rem;
	color: #db210c;
	border:1px solid #db210c;
	border-radius: 3px;
	background: #fff;
	text-align: center;
	line-height: .4rem;

}
.page_book a:first-child{
	float:left;
}
.page_book a:last-child{
	float:right;
}
/*分页技术*/


/*账单明细*/
.acc_box{
	margin-top:.14rem;
	background: #fff;
}
.z_d{
	height:.74rem;
	line-height: .74rem;
	border-bottom:1px solid #f6f6f6;
	padding:0 .34rem;
}
.z_d p{
	font-size:.24rem;
	color:#282828;
}



.z_list .z_tit li{
	position: relative;
	top:0;
	height:.74rem;
	line-height: .74rem;
	width:20%;
	text-align: center;
	float:left;
	font-size:.22rem;
	color:#282828;
	background: #fff;
}
.z_list .z_tit  li img{
	width:.14rem;
	height:.08rem;
	margin-left: .04rem;
}

/*头部结束*/



/*列表位置*/
.z_li ul li{
	white-space:nowrap;overflow:hidden;text-overflow: ellipsis;
	width:20%;
	text-align: center;	
	height:.74rem;
	line-height: .74rem;
	float:left;
	font-size:.18rem;
	color:#282828;
	background: #fff;
}
.z_li ul li span{
	font-size:.14rem;
}

.z_heji ul li{
	height:.86rem;
	background: #F3001D;
	border: none;
	font-size:.28rem;
	line-height:.86rem;
	color:#fff;
	font-weight: 100;
}
/*列表位置结束*/


/*交易类型下拉弹窗效果*/
.m_box{
	clear: both;
	position:absolute;
	width:2.06rem;
	border-radius:4px;
	background: #fff;
	box-shadow:0px 1px 10px #000000;
	z-index: 22;
	display: none;
	cursor: pointer;

}
.m_box span{
	position:absolute;
	left:.4rem;
	top:-.28rem;
	z-index: 11;
	border-left:10px solid transparent;
	border-top:10px solid transparent;
	border-right:10px solid transparent;
	border-bottom:10px solid #fff ;
}
.m_box div{
	width:100%;
	height:.54rem;
	font-size:.2rem;
	color:#282828;
	line-height: .54rem;
	border-bottom:1px solid #ccc;
	text-align: left;
	text-indent: .14rem;
	overflow: hidden;
	white-space: normal;
	text-overflow:ellipsis;
}
.m_box div:last-child{
	border-bottom:none;
}

/*普通会员我的推荐页面*/
