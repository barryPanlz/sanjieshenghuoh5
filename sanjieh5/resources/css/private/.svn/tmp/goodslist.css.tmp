.main1{
	overflow: hidden;
	 float: left;
}
.main .cl_top{
	width: 100%;
	height: 0.7rem;
	overflow: hidden;
	background: #FFFFFF;
	border-bottom: 1px solid #f6f6f6;
}
.cl_top_div{
	width: 25%;
	height: 0.7rem;
	float: left;
	text-align: center;
	line-height: 0.7rem;
	font-size: 0.24rem;
	position: relative;
}
.cl_top_div img{
	
	width: 0.12rem;
}
.cl_top_div a{
	position: absolute;
	right: 0.2rem;
	top: 0.26rem;
	display: inline-block;
	width: 0.14rem;
	height: 0.18rem;
}
.cl_top_div a img{
	width: 0.12rem;
	float: left;
}
.cl_top_div a img:nth-of-type(2){
	margin-top:0.04rem ;
}
.cl_top_div_p{
	width: 1.34rem;
	height: 0.5rem;
	background: #f0f3f5;
	margin: 0.09rem auto;
	text-align: center;
	line-height: 0.5rem;
	border-radius:3px ;
	font-size:0.24rem ;
	color: #000000;
}
.cl_top_ul{
	display: none;
	position: absolute;
	width: 100%;
	height: 8.35rem;
	left: 0;
	top: 1.4rem;
	background: rgba(1,1,1,0.4);
	z-index: 100;
}
.cl_top_ul li{
	width: 100%;
	min-height: 0.2rem;
	background: #FFFFFF;
	float: left;
}
.cl_top_ul li:nth-of-type(1){
	border-top:1px solid #DDDDDD ;
	border-bottom:1px solid #DDDDDD ;
}
.cl_top_ul li a{
	display: inline-block;
	width: 32%;
	height: 0.58rem;
	line-height:0.58rem ;
	text-align: center;
	float: left;
	font-size: 0.2rem;
}
.cl_top_ul li button{
	width: 50%;
	height: 0.9rem;
	float: left;
	background: #ffffff;
	font-size: 0.24rem;
	color: #282828;
	text-align: center;
	line-height: 0.9rem;
}
.cl_top_ul li button:nth-of-type(2){
	background: #f3001d;
	color: #FFFFFF;
}
.fun_cl_tab{
	border: 1px solid #d4d4d4;
	border-bottom:0 ;
	background: #ffffff;
	height: 0.59rem;
	position: relative;
	z-index: 9999;
	border-radius:3px 3px 0 0 ;
}

.cl_click_color span{
	color: #f3001d;
}
/*center*/
.cl_center{
	width: 100%;
	/*height: 6.4rem;*/
	position: absolute;
	top: 1.4rem;
	left: 0;
	bottom: 0;
	background: #FFFFFF;
	overflow-x: hidden;
	overflow-y:scroll ;
}
.cl_center ul{
	overflow-x: hidden;
	float: left;
	width: 100%;
	/*height: 6.4rem;*/
	float: left;
	/*padding-bottom:2rem ;*/
	
}
.cl_center li{
	width: 100%;
	height: 2rem;
	float: left;
	padding: 0.2rem 0.16rem;
	border-bottom:1px solid #D4D4D4 ;
}
.cl_center li a{
	width: 100%;
	height: 100%;
}
.cl_center li img{
	width: 31%;
	height:2rem;
	float: left;
}
.cl_center li .cl_center_right{
	width: 60%;
	height: 100%;
	float: left;
	margin-left:0.1rem ;
}
.cl_center_right p:nth-of-type(1){
	font-size: 0.24rem;
	width: 100%;
	height: 0.8rem;
	float: left;
	line-height: 0.4rem;
	color: #282828;
	overflow: hidden;
 	text-overflow: ellipsis;
 	display: -webkit-box;
 	-webkit-line-clamp: 2;
 	-webkit-box-orient: vertical;
}
.cl_center_right .cl_center_rig_p2{
	width: 100%;
	height:auto;
	float: left;
	margin-top:0.2rem ;
	font-size: 0;
}
.cl_center_rig_p2 span{
	font-size: 0.3rem;
	color: #F3001D;
}
.cl_center_rig_p2 span:nth-of-type(2){
	font-size:0.24rem ;
	margin-left:0.12rem ;
	margin-top:0.08rem ;
}
.cl_center_rig_p2 em{
	font-style: normal;
	font-size: 0.18rem;
	color: #999999;
}
.cl_center_rig_p2 em:nth-of-type(2){
	color: #282828;
	margin-left:0.08rem ;
}
